{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "ES2018"
      ],
      "target": "es5",
      "module": "CommonJS",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "outDir": "./build",
      "resolveJsonModule": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "baseUrl": "./src"
   },
   "exclude": ["node_modules"]
}